{
  "manifest_version": 2,

  "name": "WindowDotLocation",
  "description": "Shares your location on the web with everyone.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery-3.1.1.min.js", 
        "socket.js", 
        "helpers.js", 
        "viewGen.js",
        "main.js"
      ]
    }
  ],
  "permissions": ["storage", "tabs"]
}